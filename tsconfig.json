{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@cv": [
        "./cv.json"
      ],
      "@cv/*": [
        "./cv.*.json"
      ],
      "@/*": [
        "./src/*"
      ],
      "@astro.config": [
        "./astro.config.mjs"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}